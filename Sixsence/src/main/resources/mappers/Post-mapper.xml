<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.six.mapper.PostMapper">
<!-- select 전체 게시글 보기 SQL -->
 <select id="findAll" resultType="com.six.dto.Post">
    SELECT post_no, post_title, post_content, post_count, post_createDate
    FROM post
  </select>
   
   <!-- post 게시글 테이블에 글 작성 
   <insert id="insertPost" parameterType="com.six.dto.Post">
   INSERT INTO post (post_title,post_content,post_count)
   VALUES(#{postTitle},#{postContent),#{postCount})
</insert>
-->
</mapper>